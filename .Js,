import { useEffect } from "react";

export default function UnemploymentDownload() {
  useEffect(() => {
    window.location.href = "/docs/unemployment.pdf";
  }, []);

  return (
    <div style={{ padding: 24, fontFamily: "Arial, sans-serif" }}>
      <h1>Downloading…</h1>
      <p>Si telechajman pa kòmanse, <a href="/docs/unemployment.pdf">klike isit la</a>.</p>
      <p><a href="/">← Retounen lakay</a></p>
    </div>
  );
}
